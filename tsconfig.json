{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": { "@/*": ["src/*"] },
    "types": ["vite/client"]
    // "allowJs": true,
    // "checkJs": false
  },
  "include": ["src/JavaScript/storage/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
